# Environment
ENVIRONMENT=development
LOG_LEVEL=INFO

# AWS Configuration
AWS_REGION=us-east-1

# AgentCore Resources (Get these from Terraform outputs)
CODE_INTERPRETER_ID=your-code-interpreter-id-here
BROWSER_ID=your-browser-id-here

# AgentCore Memory Configuration
# Set MEMORY_ID to enable conversation memory (leave empty to disable)
MEMORY_ID=

# Memory Session Configuration (optional, uses defaults if not specified)
DEFAULT_SESSION_ID=default-session
DEFAULT_ACTOR_ID=default-user
